		reg = <0x55 0>;
		qcom,modify-soc-smooth;
		//qcom,batt_bq28z610 = <1>;
		//qcom,sha1_key_index = <1>;
		//qcom,bq28z610_need_balancing;
	};

	da9313-divider@68 {
