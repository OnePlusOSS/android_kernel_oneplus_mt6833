/*VENDOR_EDIT*/
&i2c6 {
    tfa98xx: tfa98xx {
        compatible = "nxp,tfa98xx";
        reg = <0x34>;
        #sound-dai-cells = <0>;
        reset-gpio = <&pio 139 0>;
        tfa_min_range = <5000>;
        tfa_max_range = <8000>;
        status = "okay";
    };

    fsa4480: fsa4480@42 {
        compatible = "qcom,fsa4480-i2c";
        reg = <0x42>;
        pinctrl-names = "hs_det_gpio_low","hs_det_gpio_high";
        pinctrl-0 = <&fsa_hs_det_low>;
        pinctrl-1 = <&fsa_hs_det_high>;
        fsa4480,hs-det-gpio = <&pio 128 0>;
    };
};
